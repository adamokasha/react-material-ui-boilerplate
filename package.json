{
  "name": "react-boilerplate-mui",
  "version": "1.0.0",
  "sideEffects": false,
  "description": "React Boilerplate with Webpack 4 and Material UI component set",
  "main": "index.js",
  "scripts": {
    "dev": "webpack --env development --mode=development",
    "test": "cross-env NODE_ENV=test jest --config=jest.config.json",
    "dev-server": "webpack-dev-server --env development --mode=development --open",
    "build": "webpack  --env production --mode=production",
    "start": "node server/server.js",
    "heroku-postbuild": "npm run build"
  },
  "author": "Sam Okasha",
  "license": "MIT",
  "devDependencies": {
    "cross-env": "^5.2.0",
    "dotenv": "^6.0.0",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.4",
    "jest": "^23.2.0",
    "react-test-renderer": "^16.4.1",
    "uglifyjs-webpack-plugin": "^2.0.0",
    "webpack-bundle-analyzer": "^3.0.2",
    "webpack-dev-server": "^3.1.4",
    "webpack-cli": "^3.0.8"
  },
  "dependencies": {
    "@material-ui/core": "^3.0.3",
    "@material-ui/icons": "^3.0.1",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "express": "^4.16.3",
    "html-loader": "^0.5.5",
    "html-webpack-plugin": "^3.2.0",
    "raf": "^3.4.0",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-router": "^4.3.1",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "webpack": "^4.4.0"
  }
}
